<sdf version='1.5'>
  <model name='iris_odom'>
    <include>
      <uri>model://iris</uri>
    </include>

    <plugin name="odometry_ground_truth" filename="libgazebo_ros_p3d.so">
      <frameName>map</frameName>
      <bodyName>iris::base_link</bodyName>
      <topicName>trueOdom</topicName>
      <updateRate>100.0</updateRate>
    </plugin>

  </model>
</sdf>
